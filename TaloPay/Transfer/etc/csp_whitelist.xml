    <policies>
        <policy id="script-src">
            <values>
                <value id="talopay-dev-pluginjs" type="host">https://talo-public-res.s3.us-east-1.amazonaws.com/magento/dev/talo-cvus-front.js</value>
                <value id="talopay-dev-api" type="host">https://dev-api.talo.com.ar</value>

                <!-- prod -->
